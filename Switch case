#include<stdio.h>
main()
{
	printf("Pick an item : \n1. French Fries, Rs 99\n2. Burger, Rs 129\n3. Sandwich, Rs 149\n4. Pasta, Rs 179\n5. Pizza Rs 239");
	int choice=0;
	scanf("%d", &choice);
	switch(choice)
	{
		case 1:
			printf("You picked French Fries, Rs 99 ");
			break;
		case 2:
			printf("You picked Burger, Rs 129 ");
			break;
		case 3:
			printf("You picked Sandwich, Rs 149");
			break;
		case 4:
			printf("You picked Pasta, Rs 179 ");
			break;
		case 5:
			printf("You picked Pizza Rs 239 ");
		default : printf("Invalid Choice");
	}
}
